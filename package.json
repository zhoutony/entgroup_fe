{
  "name": "entgroup",
  "version": "0.0.1",
  "description": "entgroup",
  "scripts": {
    "init": "npm install --registry=https://registry.npm.taobao.org && npm run clean",
    "clean": "rimraf ./build",
    "development-server": "better-npm-run development-server",
    "development-build": "npm run init && better-npm-run development-build",
    "development-build-gulp": "gulp watch",
    "development-build-webpack": "webpack -w -c --display-error-details",
    "production-server": "better-npm-run production-server",
    "production-build": "npm run init && better-npm-run production-build",
    "production-build-gulp": "gulp",
    "production-build-webpack": "webpack"
  },
  "betterScripts": {
    "development-build": {
      "command": "run-p development-build-gulp development-build-webpack",
      "env": {
        "NODE_ENV": "development",
        "PORT": "3000"
      }
    },
    "development-server": {
      "command": "nodemon ./bin/www",
      "env": {
        "NODE_ENV": "development",
        "PORT": "3000"
      }
    },
    "production-build": {
      "command": "npm run production-build-gulp && npm run production-build-webpack",
      "env": {
        "NODE_ENV": "production",
        "PORT": "3000"
      }
    },
    "production-server": {
      "command": "node ./bin/www",
      "env": {
        "NODE_ENV": "production",
        "PORT": "3000"
      }
    }
  },
  "dependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-runtime": "^6.9.2",
    "better-npm-run": "0.0.8",
    "body-parser": "^1.15.1",
    "cookie-parser": "^1.4.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.0",
    "express": "^4.13.4",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.13.0",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.1",
    "gulp-less": "^3.1.0",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^2.2.3",
    "json-loader": "^0.5.4",
    "less-plugin-autoprefix": "^1.5.1",
    "lodash": "^4.12.0",
    "moment": "^2.13.0",
    "morgan": "^1.7.0",
    "nodemon": "^1.9.2",
    "npm-run-all": "^2.1.1",
    "rimraf": "^2.5.3",
    "url-loader": "^0.5.7",
    "urlencode": "^1.1.0",
    "webpack": "^1.13.0"
  }
}
